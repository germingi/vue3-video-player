{
  "name": "@germingi/vue3-video-player",
  "description": "Vue 3 video player component with subtitle support and theme configuration.",
  "private": false,
  "version": "1.1.2",
  "keywords": [
    "vue",
    "vue3",
    "component",
    "video",
    "video player",
    "subtitles",
    "mp4",
    "theme"
  ],
  "license": "MIT",
  "homepage": "https://github.com/germingi/vue3-video-player#readme",
  "bugs": {
    "url": "https://github.com/germingi/vue3-video-player/issues"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "browser": "./dist/video-player.umd.cjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/germingi/vue3-video-player"
  },
  "module": "./dist/video-player.js",
  "exports": {
    ".": {
      "types": "./dist/video-player.d.ts",
      "import": "./dist/video-player.js",
      "require": "./dist/video-player.umd.cjs"
    },
    "./style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "format": "prettier --write src/",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@germingi/scroll-and-fade-text": "^1.0.6",
    "@heroicons/vue": "^2.2.0",
    "sanitize-html": "^2.14.0",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.13.5",
    "@types/sanitize-html": "^2.13.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/coverage-v8": "^3.0.6",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.4.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.21.0",
    "eslint-plugin-vue": "^9.32.0",
    "jsdom": "^26.0.0",
    "prettier": "^3.5.1",
    "typescript": "~5.7.2",
    "vite": "^6.1.0",
    "vite-plugin-dts": "^4.5.0",
    "vitest": "^3.0.6",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.2.0"
  }
}
